extends ./../layout
block content
	.table
		.row
			.cell(id='divServicesProfilesAvailable')
				ul
				each sp in servicesProfilesAvailable
					li
						a(href='#', onclick='navigate(\'divServicesProfilesContent\',\'manage_#{sp.prefix}\')')= sp.name
			.cell(id='divServicesProfilesContent')
			.cell(id='divServicesProfilesContentBull')
	script.
		var serviceToLoad = !{JSON.stringify(serviceToLoad)};
		console.log("a"+serviceToLoad);
		if(serviceToLoad != ""){
			navigate('divServicesProfilesContent','manage_'+serviceToLoad);
		}